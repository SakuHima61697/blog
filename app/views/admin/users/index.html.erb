<div class="admin-user-index">
    <h1 class="page-title">登録済みのユーザー一覧</h1>
    <br></br>
</div>
    <table>
      <thead>
        <tr>
          <th>id</th>
          <th>名前</th>
          <th>登録日</th>
          <th>ユーザー削除</th>
        </tr>
      </thead>
      <tbody>
            <% @users.each do |user| %>
              <% if user&.admin == false %>
                <tr>
                  <td><%= user.id %></td>
                	<td><%= user.name %></td>
                	<td><%= user.created_at.strftime("%Y年%m月%d日 %H:%M:%S") %></td>
                	<td><%= link_to("ユーザー削除", "/admin/users/delete/#{user.id}", class: "btn btn-danger") %></td>
                </tr>
            <% end %>
        <% end %>
      </tbody>
    </table>
    <br></br>
    <div class="admin-user-index">
      <!--ユーザー一覧ページ表示-->
      <%= paginate @users %>
    </div>